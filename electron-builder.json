{
  "productName": "CubeDB",
  "appId": "com.isaacvr.cubedb",
  "compression": "maximum",
  "electronDownload": {
    "cache": "cache",
    "isVerifyChecksum": false
  },
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": ["ia32"]
      }
    ],
    "artifactName": "${productName}-Setup-${version}-${arch}.${ext}"
  },
  "asar": true,
  "directories": {
    "buildResources": "public/assets",
    "output": "release/"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!*.code-workspace",
    "!src/",
    "!cache/",
    "!out/",
    "!LICENSE.md",
    "!package-lock.json",
    "!pnpm-lock.yaml",
    "!yarn.lock",
    "!postcss.config.cjs",
    "!svelte.config.cjs",
    "!tailwind.config.cjs",
    "!Todo.md",
    "!tsconfig.json",
    "!tsconfig.node.json",
    "!vite.config.ts",
    "package.json",
    "dist/**/*",
    "public/assets/**/*",
    "src/index.cjs",
    "src/preload.js",
    "src/cert/*",
    "src/serverHandlers/*"
  ],
  "extraResources": [
    "src/database/algs.db",
    "src/database/tutorials.db",
    "src/database/algversion.json",
    "src/database/tutversion.json"
  ]
}
