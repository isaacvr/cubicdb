{
  "name": "cubicdb",
  "productName": "CubicDB",
  "version": "2.1.3",
  "description": "Cubing with fun",
  "homepage": "https://github.com/isaacvr/cubicdb",
  "repository": {
    "type": "git",
    "url": "https://github.com/isaacvr/cubicdb.git"
  },
  "private": true,
  "type": "module",
  "author": {
    "name": "Isaac Vega Rodriguez",
    "email": "isaacvega1996@gmail.com"
  },
  "keywords": [
    "cubicdb",
    "speed",
    "timer",
    "simulator",
    "cca",
    "sport",
    "cube",
    "rubik",
    "speedsolving",
    "speedcubing",
    "wca",
    "svelte",
    "electron",
    "nodejs",
    "typescript",
    "desktop",
    "windows"
  ],
  "main": "src/index.cjs",
  "scripts": {
    "dev": "concurrently \"npm run web\" \"electron-forge start -- --serve\"",
    "build": "vite build",
    "web": "vite dev",
    "preview": "vite preview",
    "builder": "electron-builder build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "lint:oxc": "npx oxlint -c .oxlintrc.json",
    "format": "prettier --write .",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "license": "MIT",
  "overrides": {
    "electron-rebuild": {
      "node-abi": "3.68.0"
    }
  },
  "config": {
    "forge": {
      "electronRebuildConfig": {
        "forceABI": 113
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-squirrel",
          "platforms": [
            "win32"
          ],
          "config": {
            "outputDirectory": "./release",
            "noMsi": true
          }
        }
      ]
    }
  },
  "dependencies": {
    "@electron/remote": "2.0.8",
    "@ffmpeg/ffmpeg": "0.12.10",
    "@ffmpeg/util": "0.12.1",
    "@xstate/svelte": "3.0.5",
    "ably": "1.2.45",
    "archiver": "5.3.1",
    "async-lz-string": "1.1.0",
    "axios": "1.3.4",
    "cors": "2.8.5",
    "electron-reload": "1.5.0",
    "electron-updater": "5.3.0",
    "eruda": "3.0.1",
    "express": "4.18.2",
    "fft.js": "^4.0.4",
    "flowbite-svelte-icons": "0.4.5",
    "idb": "8.0.0",
    "js-confetti": "0.11.0",
    "jspdf": "2.5.1",
    "jszip": "3.10.1",
    "katex": "0.16.10",
    "moment": "2.29.4",
    "nedb": "1.8.0",
    "node-machine-id": "1.1.12",
    "object-hash": "3.0.0",
    "qrcode": "1.5.3",
    "socket.io": "4.7.4",
    "socket.io-client": "4.7.2",
    "svelte-easy-crop": "3.0.0",
    "svelte-material-icons": "3.0.5",
    "svelte-routing": "2.13.0",
    "three": "0.165.0",
    "unzipper": "0.10.11",
    "xstate": "5.18.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@playwright/test": "1.28.1",
    "@sveltejs/adapter-auto": "3.0.0",
    "@sveltejs/kit": "2.0.0",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@tsconfig/svelte": "3.0.0",
    "@types/archiver": "5.3.1",
    "@types/audioworklet": "0.0.50",
    "@types/echarts": "4.9.22",
    "@types/eslint": "9.6.0",
    "@types/katex": "0.16.7",
    "@types/nedb": "1.8.12",
    "@types/object-hash": "3.0.4",
    "@types/qrcode": "1.5.2",
    "@types/three": "0.165.0",
    "@types/web-bluetooth": "0.0.17",
    "@vitejs/plugin-basic-ssl": "1.1.0",
    "autoprefixer": "10.4.16",
    "concurrently": "5.1.0",
    "echarts": "5.4.3",
    "electron": "32.1.2",
    "electron-builder": "24.13.3",
    "esbuild": "0.19.4",
    "eslint": "9.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.36.0",
    "flowbite": "2.2.1",
    "flowbite-svelte": "0.44.21",
    "globals": "15.0.0",
    "node-abi": "3.68.0",
    "oxlint": "0.4.4",
    "postcss": "8.4.31",
    "postcss-load-config": "4.0.1",
    "prettier": "3.1.1",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "4.2.12",
    "svelte-check": "4.0.0",
    "svelte-flag-icons": "0.7.2",
    "svelte-preprocess": "5.0.4",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.2",
    "typescript": "5.4.3",
    "typescript-eslint": "8.0.0",
    "vite": "5.2.8",
    "vite-plugin-pwa": "0.17.5",
    "vitest": "2.0.3",
    "workbox-cli": "7.0.0"
  }
}
